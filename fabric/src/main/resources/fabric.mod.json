{
	"schemaVersion": 1,
	"id": "mtrfranceaddon",
	"version": "${version}",
	"name": "MTR France Addon",
	"description": "This addon is provided to give French Railway content to players through the Minecraft Transit Railway mod by jonafanho.\nCet addon fourni un panel d'éléments du rail français aux joueurs au travers du mod Minecraft Transit Railway par jonafanho.\n\nCreated by the french community - Créer par la communauté française.",
	"authors": ["Team MTR France"],
	"contact": {
		"homepage": "https://github.com/mtr-france-team/mtr-france-addon",
		"sources": "https://github.com/mtr-france-team/mtr-france-addon",
        "issues": "https://github.com/mtr-france-team/mtr-france-addon/issues"
	},
	"license": "MIT",
	"icon": "assets/mtrfranceaddon/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": ["fr.mtrfranceaddon.fabric.MTRFranceAddonFabric"],
		"client": ["fr.mtrfranceaddon.fabric.MTRFranceAddonFabricClient"]
	},
	"mixins": ["mtrfranceaddon.mixins.json"],
	"depends": {
		"fabricloader": ">=0.15.0",
		"fabric": "*",
		"minecraft": "${minecraft_version}",
		"mtr": ">=4.0.0"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discord.gg/485Mam2fM3"
            },
          "update_checker": true
        }
    }
}
